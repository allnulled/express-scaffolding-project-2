</head>

<body>
    <%-await include(process.env.PROJECT_SRC + "/batches/templates/partials/header.ejs" , { title: "Error" })%>
    <main>
        <div id="main-contents">
            <div id="main-contents-box">
                <div id="error-page-message">
                    <p><%-errorMessage || "We are sorry. Please, be patient. Some error happened, and we will take care of it." %></p>
                </div>
            </div>
        </div>
        <%-await include(process.env.PROJECT_SRC + "/batches/templates/partials/aside.ejs" )%>
    </main>
    <%-await include(process.env.PROJECT_SRC + "/batches/templates/partials/footer.ejs" )%>
</body>

</html>